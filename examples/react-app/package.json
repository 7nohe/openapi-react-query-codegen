{
  "name": "@7nohe/react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:mock dev:client",
    "dev:client": "vite --clearScreen=false",
    "dev:mock": "prism mock ../petstore.yaml --dynamic",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate:api": "rimraf ./openapi && node ../../dist/cli.mjs -i ../petstore.yaml -c @hey-api/client-fetch --format=biome --lint=biome",
    "test:generated": "tsc -p ./tsconfig.openapi.json --noEmit"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.1.3",
    "@tanstack/react-query": "^5.32.1",
    "axios": "^1.6.7",
    "form-data": "~4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.2",
    "@stoplight/prism-cli": "^5.5.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "vite": "^5.0.12"
  }
}
