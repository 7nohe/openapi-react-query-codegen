{
  "name": "@7nohe/react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:mock dev:client",
    "dev:client": "vite --clearScreen=false",
    "dev:mock": "prism mock ./petstore.yaml --dynamic",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate:api": "rimraf ./openapi && node ../../dist/cli.mjs -i ./petstore.yaml -c axios --request ./request.ts",
    "test:generated": "tsc -p ./tsconfig.openapi.json --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.18.1",
    "axios": "^1.6.7",
    "form-data": "~4.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^5.5.2",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
